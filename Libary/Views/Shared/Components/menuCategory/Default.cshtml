@model IEnumerable<Libary.ViewModels.menuCategory_ViewModels>
<div class="col-lg-12">
    <div class="mb-3">
        <h4>Categories</h4>
        <ul class="list-unstyled fruite-categorie">
            <li>
                @foreach (var item in Model)
                {
                    <div class="d-flex justify-content-between fruite-name">
                        <a asp-controller="listBook" asp-action="categoryBook" asp-route-category="@item.categoryID">
                            <i class="bi bi-book"></i>  @item.categoryName
                        </a>
                    </div>
                }
            </li>
        </ul>
    </div>
</div>